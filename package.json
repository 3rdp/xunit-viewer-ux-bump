{
  "name": "xunit-viewer",
  "version": "6.0.0-alpha.4",
  "description": "parses xunit xml into xunit viewer",
  "repository": {
    "url": "https://github.com/lukejpreston/xunit-viewer.git",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/lukejpreston/xunit-viewer.git"
  },
  "homepage": "./",
  "keywords": [
    "test",
    "junit",
    "xunit",
    "viewer",
    "parser"
  ],
  "author": "lukejpreston <lukejpreston@gmail.com>",
  "license": "MIT",
  "main": "xunit-viewer",
  "bin": {
    "xunit-viewer": "bin/xunit-viewer"
  },
  "directories": {
    "bin": "bin",
    "cli": "cli"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src index.js",
    "deploy": "npm run build && cp -r build/static cli/static"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "change-case": "^3.1.0",
    "chokidar": "^3.0.2",
    "codemirror": "^5.48.2",
    "console-clear": "^1.1.1",
    "debounce": "^1.2.0",
    "fuzzy": "^0.1.3",
    "get-port": "^5.0.0",
    "handlebars": "^4.1.2",
    "ip": "^1.1.5",
    "koa": "^2.8.1",
    "merge": "^1.2.1",
    "react-burgers": "^1.3.0",
    "react-codemirror2": "^6.0.0",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0",
    "xml2js": "^0.4.19",
    "yargs": "^13.3.0"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.10.1",
    "bulma": "^0.7.5",
    "eslint": "^6.1.0",
    "eslint-config-standard": "^13.0.1",
    "eslint-config-standard-react": "^8.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-standard": "^4.0.0",
    "pm2": "^3.5.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-scripts": "^3.1.1",
    "react-test-renderer": "^16.9.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "eslintConfig": {
    "extends": [
      "standard",
      "standard-react"
    ],
    "env": {
      "browser": true,
      "jest": true,
      "jasmine": true
    },
    "rules": {
      "react/prop-types": 0
    }
  }
}
