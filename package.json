{
  "name": "xunit-viewer",
  "version": "5.1.10",
  "description": "parses xunit xml into xunit viewer",
  "repository": {
    "url": "https://github.com/lukejpreston/xunit-viewer.git",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/lukejpreston/xunit-viewer.git"
  },
  "homepage": "https://lukejpreston.github.io/xunit-viewer",
  "keywords": [
    "test",
    "junit",
    "xunit",
    "viewer",
    "parser"
  ],
  "author": "lukejpreston <lukejpreston@gmail.com>",
  "license": "MIT",
  "main": "xunit-viewer",
  "bin": {
    "xunit-viewer": "bin/xunit-viewer"
  },
  "directories": {
    "bin": "bin",
    "cli": "cli",
    "component": "component",
    "config": "config",
    "parser": "parser"
  },
  "scripts": {
    "start": "SKIP_PREFLIGHT_CHECK=true react-scripts start",
    "build": "SKIP_PREFLIGHT_CHECK=true react-scripts build",
    "test": "SKIP_PREFLIGHT_CHECK=true react-scripts test",
    "eject": "SKIP_PREFLIGHT_CHECK=true react-scripts eject"
  },
  "dependencies": {
    "bulma": "^0.7.4",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "eslint": "^5.15.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-standard-react": "^7.0.2",
    "eslint-plugin-jasmine": "^2.10.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^24.3.1",
    "react-scripts": "2.1.8"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](node_modules|scripts)[/\\\\]",
      "<rootDir>/component/suites/test.jsx",
      "<rootDir>/component/unbundled.js"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "eslintConfig": {
    "extends": [
      "standard"
    ],
    "env": {
      "node": true,
      "jest": true,
      "jasmine": true
    }
  }
}
